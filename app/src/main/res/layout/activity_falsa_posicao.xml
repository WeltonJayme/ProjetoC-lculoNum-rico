<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:screenOrientation="portrait">

    <TextView
        android:id="@+id/textView_função_falsaPosição"
        android:layout_width="78dp"
        android:layout_height="29dp"
        android:layout_marginEnd="141dp"
        android:layout_marginLeft="141dp"
        android:layout_marginRight="141dp"
        android:layout_marginStart="141dp"
        android:layout_marginTop="20dp"
        android:text="@string/String_false_Funcao"
        android:textAlignment="center"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_falsaPosição"
        android:layout_width="114dp"
        android:layout_height="45dp"
        android:layout_marginBottom="48dp"
        android:layout_marginEnd="123dp"
        android:layout_marginLeft="123dp"
        android:layout_marginRight="123dp"
        android:layout_marginStart="123dp"
        android:text="BUSCAR RAIZ"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:textSize="14sp" />

    <EditText
        android:id="@+id/editText_função_falsaPosição"
        android:layout_width="178dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="91dp"
        android:layout_marginLeft="91dp"
        android:layout_marginRight="91dp"
        android:layout_marginStart="91dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="x^2 + x - 6"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_função_falsaPosição" />

    <TextView
        android:id="@+id/textView_a_falsaPosição"
        android:layout_width="31dp"
        android:layout_height="20dp"
        android:layout_marginLeft="96dp"
        android:layout_marginStart="96dp"
        android:layout_marginTop="25dp"
        android:text="@string/String_false_A"
        android:textAlignment="center"
        android:textStyle="bold"
        android:textSize="15sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/textView_b_falsaPosição"
        android:layout_width="31dp"
        android:layout_height="20dp"
        android:layout_marginEnd="100dp"
        android:layout_marginRight="100dp"
        android:layout_marginTop="25dp"
        android:text="@string/String_false_B"
        android:textAlignment="center"
        android:textStyle="bold"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <EditText
        android:id="@+id/editText_a_falsaPosição"
        android:layout_width="72dp"
        android:layout_height="50dp"
        android:layout_marginLeft="69dp"
        android:layout_marginStart="69dp"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:hint="-8"
        android:inputType="textPersonName"
        android:textAlignment="center"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_a_falsaPosição"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/editText_tol_falsaPosição"
        android:layout_width="72dp"
        android:layout_height="50dp"
        android:layout_marginLeft="69dp"
        android:layout_marginStart="69dp"
        android:layout_marginTop="19dp"
        android:ems="10"
        android:hint="1e-5"
        android:inputType="textPersonName"
        android:textAlignment="center"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_tol_muller" />

    <EditText
        android:id="@+id/editText_n_falsPosição"
        android:layout_width="72dp"
        android:layout_height="50dp"
        android:layout_marginEnd="79dp"
        android:layout_marginRight="79dp"
        android:layout_marginTop="19dp"
        android:ems="10"
        android:hint="100"
        android:inputType="textPersonName"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_n_muller" />

    <EditText
        android:id="@+id/editText_b_falsaPosição"
        android:layout_width="72dp"
        android:layout_height="50dp"
        android:layout_marginEnd="79dp"
        android:layout_marginRight="79dp"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="1.5"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_b_falsaPosição" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="196dp"
        android:layout_height="27dp"
        android:layout_marginEnd="82dp"
        android:layout_marginLeft="82dp"
        android:layout_marginRight="82dp"
        android:layout_marginStart="82dp"
        android:layout_marginTop="28dp"
        android:text="Estimativas Iniciais"
        android:textAlignment="center"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editText_função_falsaPosição" />

    <TextView
        android:id="@+id/textView_n_muller"
        android:layout_width="144dp"
        android:layout_height="18dp"
        android:layout_marginEnd="33dp"
        android:layout_marginRight="33dp"
        android:layout_marginTop="37dp"
        android:text="@string/txt_maximoIteracoes"
        android:textAlignment="center"
        android:textStyle="bold"
        android:textSize="@dimen/estimativas_size"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editText_b_falsaPosição" />

    <TextView
        android:id="@+id/textView_tol_muller"
        android:layout_width="90dp"
        android:layout_height="18dp"
        android:layout_marginLeft="60dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="37dp"
        android:text="Erro Mínimo"
        android:textAlignment="center"
        android:textStyle="bold"
        android:textSize="@dimen/estimativas_size"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editText_a_falsaPosição" />

    <Button
        android:id="@+id/btn_help_falsa_posicao"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginEnd="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/help"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</android.support.constraint.ConstraintLayout>