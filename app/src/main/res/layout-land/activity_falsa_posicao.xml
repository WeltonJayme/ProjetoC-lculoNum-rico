<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:screenOrientation="portrait">

    <TextView
        android:id="@+id/textView_função_falsaPosição"
        android:layout_width="78dp"
        android:layout_height="29dp"
        android:layout_marginEnd="373dp"
        android:layout_marginLeft="80dp"
        android:layout_marginRight="373dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="5dp"
        android:text="@string/String_false_Funcao"
        android:textAlignment="center"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_falsaPosição"
        android:layout_width="114dp"
        android:layout_height="45dp"
        android:layout_marginBottom="144dp"
        android:layout_marginEnd="100dp"
        android:layout_marginLeft="145dp"
        android:layout_marginRight="100dp"
        android:layout_marginStart="145dp"
        android:text="BUSCAR RAIZ"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:textSize="14sp" />

    <EditText
        android:id="@+id/editText_função_falsaPosição"
        android:layout_width="178dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="414dp"
        android:layout_marginLeft="120dp"
        android:layout_marginRight="414dp"
        android:layout_marginStart="120dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:hint="x^2 + x - 6"
        android:inputType="textPersonName"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_função_falsaPosição" />

    <TextView
        android:id="@+id/textView_a_falsaPosição"
        android:layout_width="31dp"
        android:layout_height="20dp"
        android:layout_marginLeft="75dp"
        android:layout_marginStart="75dp"
        android:layout_marginTop="10dp"
        android:text="@string/String_false_A"
        android:textAlignment="center"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/textView_b_falsaPosição"
        android:layout_width="31dp"
        android:layout_height="20dp"
        android:layout_marginEnd="370dp"
        android:layout_marginRight="370dp"
        android:layout_marginTop="10dp"
        android:text="@string/String_false_B"
        android:textAlignment="center"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <EditText
        android:id="@+id/editText_a_falsaPosição"
        android:layout_width="72dp"
        android:layout_height="50dp"
        android:layout_marginLeft="55dp"
        android:layout_marginStart="55dp"
        android:layout_marginTop="2dp"
        android:ems="10"
        android:hint="1.5"
        android:inputType="textPersonName"
        android:textAlignment="center"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_a_falsaPosição"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/editText_tol_falsaPosição"
        android:layout_width="72dp"
        android:layout_height="50dp"
        android:layout_marginLeft="55dp"
        android:layout_marginStart="55dp"
        android:layout_marginTop="5dp"
        android:ems="10"
        android:hint="1e-5"
        android:inputType="textPersonName"
        android:textAlignment="center"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_tol_muller" />

    <EditText
        android:id="@+id/editText_n_falsPosição"
        android:layout_width="72dp"
        android:layout_height="50dp"
        android:layout_marginEnd="350dp"
        android:layout_marginRight="350dp"
        android:layout_marginTop="5dp"
        android:ems="10"
        android:hint="100"
        android:inputType="textPersonName"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_n_muller" />

    <EditText
        android:id="@+id/editText_b_falsaPosição"
        android:layout_width="72dp"
        android:layout_height="50dp"
        android:layout_marginEnd="350dp"
        android:layout_marginRight="350dp"
        android:layout_marginTop="2dp"
        android:ems="10"
        android:hint="1.7"
        android:inputType="textPersonName"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_b_falsaPosição" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="196dp"
        android:layout_height="27dp"
        android:layout_marginEnd="396dp"
        android:layout_marginLeft="100dp"
        android:layout_marginRight="396dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="10dp"
        android:text="Estimativas Iniciais"
        android:textAlignment="center"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editText_função_falsaPosição" />

    <TextView
        android:id="@+id/textView_n_muller"
        android:layout_width="144dp"
        android:layout_height="18dp"
        android:layout_marginEnd="305dp"
        android:layout_marginRight="305dp"
        android:layout_marginTop="5dp"
        android:text="@string/txt_maximoIteracoes"
        android:textAlignment="center"
        android:textSize="@dimen/estimativas_size"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editText_b_falsaPosição" />

    <TextView
        android:id="@+id/textView_tol_muller"
        android:layout_width="90dp"
        android:layout_height="18dp"
        android:layout_marginLeft="45dp"
        android:layout_marginStart="45dp"
        android:layout_marginTop="5dp"
        android:text="Erro Mínimo"
        android:textAlignment="center"
        android:textSize="@dimen/estimativas_size"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editText_a_falsaPosição" />

    <Button
        android:id="@+id/btn_help_falsa_posicao"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginEnd="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/help"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</android.support.constraint.ConstraintLayout>